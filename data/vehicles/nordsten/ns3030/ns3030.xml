            <sound template="clackVar5" startTime="0.49" volumeScale="1.1" pitchScale="1" linkNode="attacherJoint"/>
            <sound template="latchSmall" startTime="0.49" volumeScale="0.2" pitchScale="1.6" linkNode="attacherJoint"/>
